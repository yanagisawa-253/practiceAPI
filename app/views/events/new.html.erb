<%= form_with(model: @event, local: true) do |form| %>

  <div class="title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

    <div class="time">
    <%= form.label :start %>
    <%= form.datetime_select :start %>
  </div>

  <div class="content">
    <%= form.label :end %>
    <%= form.datetime_select :end %>
  </div>


  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>